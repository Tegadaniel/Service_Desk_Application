            <td>{{SDA_CUSTOMER_FNAME}}</td>
            <td>{{SDA_CUSTOMER_EMAIL}}</td>
            <td>{{SDA_BODY_TEXT}}</td>
            <td>{{SDA_STATUS_NAME}}</td>
            <td>{{SDA_CATEGORY_NAME}}</td>
            <td>{{ formatTime SDA_BODY_DATE_CREATED}}</td>
            {{#if (checkChat SDA_STATUS_NAME)}}
            <td><button class="btn btn-primary mb-4 "data-toggle="modal" data-target="#myModal3" onclick="supportChatFunc('{{SDA_BODY_ID}}')" data-id="{{SDA_BODY_ID}}">View Conversations</button></td>
            {{/if}}
