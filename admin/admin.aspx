          {{/each}}
          </tbody>
        </table>
      </div>

    </script>
    <!-- handlebar for viewChat modal  -->
    <script id="supportChatHistory" type="text/x-handlebars">

          <div class="card direct-chat direct-chat-primary">
            <div class="card-header">
              <h3 class="card-title">Direct Chat</h3>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
        
          
          <div class="card-body">
            
            <div class="direct-chat-messages">
              {{#with OUTPUT.[0]}}
              <div class="direct-chat-msg">
                <div class="direct-chat-infos clearfix">
                  <span class="direct-chat-name float-left">{{SDA_CUSTOMER_FNAME}} (Customer)</span>
                  <span class="direct-chat-timestamp float-right">{{formatTime SDA_DATE_CREATED}}</span>
                </div>
                <div class="direct-chat-text">
                  {{SDA_BODY_TEXT}}
                </div>
              </div>
              {{/with}}
              {{#each OUTPUT}}
              {{#if (chattype RESPOND_TYPE)}}
              <div class="direct-chat-msg">
              <div class="direct-chat-infos clearfix">
                <span class="direct-chat-name float-left">{{RESPONDER}}</span>
                <span class="direct-chat-timestamp float-right">{{formatTime SUPPORT_REPLY_DATE_CREATED}}</span>
              </div>
              <div class="direct-chat-text">
                {{SUPPORT_REPLY_BODY}}
              </div>
            </div>
            {{else}}
        
            <div class="direct-chat-msg right">
              <div class="direct-chat-infos clearfix">
                <span class="direct-chat-name float-right">{{RESPONDER}}</span>
                <span class="direct-chat-timestamp float-left">{{formatTime SUPPORT_REPLY_DATE_CREATED}}</span>
              </div>
              <div class="direct-chat-text">
                {{SUPPORT_REPLY_BODY}}
              </div>
            </div>
            {{/if}}
            {{/each}}
            <div class="modal-footer">
              <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
              {{#with OUTPUT.[0]}}
              <button type="button" class="btn btn-primary" data-toggle="modal" onclick="customerProfileChecker('{{SDA_CUSTOMER_ID}}')" data-customer_id="{{SDA_CUSTOMER_ID}}" data-target="#myModalProfile">View Customer Profile</button>
              {{/with}}
            </div>
          </div>
          </div>
        </div>
        
        </script>
