              <h3 class="card-title">Close a request</h3>
            </div>
            <div class="card-body">
              <div id="adminCloseReq">

              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- /.content -->
    </div>
    <!-- handlebar for close request -->
    <script id="adminCloseRequest" type="text/x-handlebars">
      <div class="table-responsive">
        <table class="table table-dark pt-5">
          <thead>
            <tr>
              <th scope="col">Customer's Name</th>
              <th scope="col">Email</th>
              <th scope="col">Complain</th>
              <th scope="col">Category</th>
              <th scope="col">Status</th>
              <th scope="col">Date Created</th>
      
            </tr>
          </thead>
          
          <tbody id="tableRow">
            {{#each OUTPUT}}
            <tr>
              <td>{{SDA_CUSTOMER_FNAME}}</td>
              <td>{{SDA_CUSTOMER_EMAIL}}</td>
              <td>{{SDA_BODY_TEXT}}</td>
              <td>{{SDA_CATEGORY_NAME}}</td>
              <td>{{SDA_STATUS_NAME}}</td>
              <td>{{formatTime SDA_BODY_DATE_CREATED}}</td>
              <td><button class="btn btn-primary mb-4 " onclick="changeComplainStatus('{{SDA_BODY_ID}}')" data-id="{{SDA_BODY_ID}}">Close Request</button></td>
            </tr>
              {{/each}}
          </tbody>
        </table>
      </div>
  
</script>


    <!-- Main Footer -->
    <footer class="main-footer">
      <!-- Default to the left -->
      <strong> <a id="admin">SDA</a>.</strong> All rights reserved.
    </footer>
  </div>
  <!-- ./wrapper -->

  <!-- REQUIRED SCRIPTS -->
  <script src="../Scripts/handlebars.js"></script>
  <!-- jQuery -->
  <script src="../plugins/jquery/jquery.min.js"></script>
  <!-- Bootstrap 4 -->
  <script src="../plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
  <!-- AdminLTE App -->
  <script src="../dist/js/adminlte.min.js"></script>
  <script>
    Handlebars.registerHelper('formatTime', function (date) {
      return new Date(date).toDateString();
    });
    Handlebars.registerHelper('statusCheck', function (value) {
      var b = true;
      if (value != "Pending" && value != "Customer Reply") {
        b = false;
      }
      return b;
    });
    Handlebars.registerHelper('checkChat', function (value) {
      var b = false;
      if (value != "Pending" && value != "Customer Reply") {
        b = true;
      }
      return b;
    });
    Handlebars.registerHelper('chattype', function (val) {
      return (val == "CUSTOMER") ? 1 : 0;
    });
  </script>
